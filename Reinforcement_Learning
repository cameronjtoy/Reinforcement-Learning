   "cell_type": "code",
   "execution_count": 32,
   "id": "db96d364",
   "metadata": {},
   "outputs": [],
   "source": [
    "def finalQTableQLearning(epsilon, epsilonDecay, total_episodes, alpha, gamma):\n",
    "    env = GridEnvironment()\n",
    "    qTable = np.zeros([25,4])\n",
    "    arr = {}\n",
    "    epsilonDic = {}\n",
    "    for i in range(total_episodes):\n",
    "        env.reset()\n",
    "        currentNum = 0\n",
    "        state = (5 * env.agent_pos[0]) + env.agent_pos[1]\n",
    "        done = False \n",
    "        totalreward=0\n",
    "        while not done and currentNum < env.max_timesteps:\n",
    "            action = env.action_space.sample() if np.random.uniform(0, 1) < epsilon else np.argmax(qTable[state])\n",
    "            obs, reward, done, info = env.step(action)\n",
    "            totalreward += reward\n",
    "            nextState = (5 * env.agent_pos[0]) + env.agent_pos[1]\n",
    "            loss = (reward + gamma * np.max(qTable[nextState]) - qTable[state, action])\n",
    "            qTable[state, action] = qTable[state, action] + alpha * loss\n",
    "            state = (5 * env.agent_pos[0]) + env.agent_pos[1]\n",
    "            currentNum += 1\n",
    "        epsilon = epsilon * epsilonDecay\n",
    "        arr[i] = totalreward\n",
    "        epsilonDic[i] = epsilon\n",
    "        \n",
    "    #Evaluation\n",
    "    evalu = {}\n",
    "    for x in range(50):\n",
    "        env.reset()\n",
    "        currentNum = 0\n",
    "        state = (5 * env.agent_pos[0]) + env.agent_pos[1]\n",
    "        action = np.argmax(qTable[state])\n",
    "        done = False\n",
    "        totalreward=0\n",
    "        while not done and currentNum < env.max_timesteps:\n",
    "            obs, reward, done, info = env.step(action)\n",
    "            totalreward += reward\n",
    "            nextAction = np.argmax(qTable[state])\n",
    "            state = (5 * env.agent_pos[0]) + env.agent_pos[1]\n",
    "            action = nextAction \n",
    "            currentNum += 1\n",
    "        evalu[x] = totalreward\n",
    "    return qTable, arr, epsilonDic, evalu"
   ]
  },
