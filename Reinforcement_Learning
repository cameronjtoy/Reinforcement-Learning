    "epsilon = 1\n",
    "epsilonDecay = .9777\n",
    "total_episodes = 1000\n",
    "alpha = 0.85\n",
    "gamma = 0.95\n",
    "tempQTable, arrTotal , epsilonVals, evalu = finalQTable(epsilon, epsilonDecay, total_episodes, alpha, gamma)\n",
    "\n",
    "plt.figure(figsize=(5,5))\n",
    "x = [0,100,200,300,400,500,600,700,800,900]\n",
    "y = [arrTotal[i] for i in x]\n",
    "plt.title(\"Total Rewards vs Episodes\")\n",
    "plt.xlabel(\"Episodes (intervals of 100)\")\n",
    "plt.ylabel(\"Total Rewards\")\n",
    "plt.plot(arrTotal.keys(),arrTotal.values())\n",
    "plt.show()\n",
    "\n",
    "plt.figure(figsize=(5,5))\n",
    "plt.title(\"Epsilons vs Episodes\")\n",
    "plt.xlabel(\"Episodes\")\n",
    "plt.ylabel(\"Epsilon\")\n",
    "plt.plot(epsilonVals.keys(),epsilonVals.values())\n",
    "plt.show()\n",
    "\n",
    "plt.figure(figsize=(5,5))\n",
    "plt.title(\"Evaluation Sarsa\")\n",
    "plt.xlabel(\"Episodes\")\n",
    "plt.ylabel(\"Evaluation of QTabel Sarsa\")\n",
    "plt.plot(evalu.keys(),evalu.values())\n",
    "plt.show()"
   ]
