   "outputs": [],
   "source": [
    "class GridEnvironment(gym.Env):\n",
    "    def __init__(self):\n",
    "        #Size of the board n x m\n",
    "        self.observation_space = spaces.Discrete(25)\n",
    "        #number of actions (up, down, left ,right)\n",
    "        self.action_space = spaces.Discrete(4)\n",
    "        #Max number of move agent wil run\n",
    "        self.max_timesteps = 9\n",
    "        \n",
    "    def reset(self):\n",
    "        self.timestep = 0\n",
    "        self.agent_pos = [0,0]\n",
    "        self.goal_pos = [4,4]\n",
    "        self.grid_reward = 0\n",
    "        self.acc = 0\n",
    "        #rewards of the board \n",
    "        self.move1=[4,2]\n",
    "        self.move2=[1,1]\n",
    "        self.move3=[1,2]\n",
    "        self.move4=[3,2]\n",
    "        self.move5=[4,0]\n",
    "        self.move6=[0,4]\n",
    "        self.state = np.zeros((5,5))\n",
    "        self.state[tuple(self.agent_pos)] = 1\n",
    "        self.state[tuple(self.goal_pos)] = 0.5\n",
    "        self.state[tuple(self.move1)] = 0.85    \n",
    "        self.state[tuple(self.move2)] = 0.85\n",
    "        self.state[tuple(self.move3)] = 0.85\n",
    "        self.state[tuple(self.move4)] = 0.85\n",
    "        self.state[tuple(self.move5)] = -0.65\n",
    "        self.state[tuple(self.move6)] = -0.65\n",
    "        observation = self.state.flatten()\n",
    "        return observation\n",
    "    \n",
    "    def step(self, action):\n",
    "        if action ==0: #down\n",
    "            self.agent_pos[0] +=1  #[0,1]->[1,1]\n",
    "        if action ==1: #up\n",
    "            self.agent_pos[0] -=1  #[1,1]->[0,1]\n",
    "        if action ==2: #right\n",
    "            self.agent_pos[1] +=1\n",
    "        if action ==3: #left\n",
    "            self.agent_pos[1] -=1    \n",
    "            \n",
    "        self.agent_pos = np.clip(self.agent_pos, 0, 4)\n",
    "        self.state = np.zeros((5,5))\n",
    "        self.state[tuple(self.agent_pos)] = 1\n",
    "        self.state[tuple(self.goal_pos)] = 0.5\n",
    "        self.state[tuple(self.move1)] = 0.85    \n",
    "        self.state[tuple(self.move2)] = 0.85\n",
    "        self.state[tuple(self.move3)] = 0.85\n",
    "        self.state[tuple(self.move4)] = 0.85\n",
    "        self.state[tuple(self.move5)] = -0.65\n",
    "        self.state[tuple(self.move6)] = -0.65\n",
    "        observation = self.state.flatten()\n",
    "        if (self.agent_pos == self.move1).all() or (self.agent_pos == self.move2).all() or (self.agent_pos == self.move3).all() or (self.agent_pos == self.move4).all():\n",
    "            self.grid_reward = 5\n",
    "            self.acc += 5\n",
    "        elif (self.agent_pos == self.move5).all() or (self.agent_pos == self.move6).all():\n",
    "            self.grid_reward = -3\n",
    "            self.acc -= 3\n",
    "        elif (self.agent_pos == self.goal_pos).all():\n",
    "            self.grid_reward = 15\n",
    "            self.acc += 15\n",
    "        else:\n",
    "            self.grid_reward = 0\n",
    "        self.timestep += 1\n",
    "        done = True if self.timestep >= self.max_timesteps or (self.agent_pos == self.goal_pos).all() else False\n",
    "        info = {}\n",
    "        \n",
    "        return observation, self.grid_reward, done, info\n",
    "    \n",
    "    \n",
    "    def render(self):\n",
    "        plt.imshow(self.state)\n",
    "        plt.show(block=True)"
   ]
  },
