   "metadata": {},
   "outputs": [],
   "source": [
    "class RandomAgent:\n",
    "    '''Random Agent class that contains the contains the information of the agent that takes action randomly'''\n",
    "    def __init__(self, env):\n",
    "        self.env = env\n",
    "        self.observation_space = env.observation_space\n",
    "        self.action_space = env.action_space\n",
    "\n",
    "    def step(self, observation):\n",
    "        return np.random.choice(self.action_space.n)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "id": "31baae77",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "import time\n",
    "# Random Agent\n",
    "# Create Environment\n",
    "env = GridEnvironment()\n",
    "\n",
    "# Create random agent\n",
    "agent = RandomAgent(env)\n",
    "obs = env.reset()\n",
    "done = True\n",
    "env.render()\n",
    "\n",
    "# Repeat steps of Agent till Episode ends\n",
    "# 0 - Right, 1 - Left, 2 - Up, 3 - Down\n",
    "while not done:\n",
    "    action = agent.step(obs)\n",
    "    obs, reward, done, _ = env.step(action)\n",
    "    time.sleep(1) \n",
    "    print('Action', action)\n",
    "    print('Reward', reward)\n",
    "    print('Done', done)\n",
    "    print()\n",
    "    env.render()"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "e9b05038",
   "metadata": {},
   "source": [
    "0 -> Down <br>\n",
    "1 -> Up <br>\n",
    "2 -> Right <br>\n",
    "3 -> Left"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
   "id": "f5e2533e",
   "metadata": {},
   "outputs": [],
   "source": [
    "def finalQTable(epsilon, epsilonDecay, total_episodes, alpha, gamma):\n",
    "    env = GridEnvironment()\n",
    "    qTable = np.zeros([25,4])\n",
    "    arr = {}\n",
    "    epsilonDic = {}\n",
    "    for i in range(total_episodes):\n",
    "        env.reset()\n",
    "        currentNum = 0\n",
    "        state = (5 * env.agent_pos[0]) + env.agent_pos[1]\n",
    "        action = env.action_space.sample() if np.random.uniform(0, 1) < epsilon else np.argmax(qTable[state])\n",
    "        done = False\n",
    "        totalreward=0\n",
    "        while not done:\n",
    "            obs, reward, done, info = env.step(action)\n",
    "            totalreward += reward\n",
    "            nextAction = env.action_space.sample() if np.random.uniform(0, 1) < epsilon else np.argmax(qTable[state])\n",
    "            loss = (reward + gamma * qTable[(5 * env.agent_pos[0]) + env.agent_pos[1], nextAction] - qTable[state, action])\n",
    "            qTable[state, action] = qTable[state, action] + alpha * loss\n",
    "            state = (5 * env.agent_pos[0]) + env.agent_pos[1]\n",
    "            action = nextAction \n",
    "            currentNum += 1\n",
    "        epsilon = epsilon * epsilonDecay\n",
    "        arr[i] = totalreward\n",
    "        epsilonDic[i] = epsilon\n",
    "        \n",
    "    #Evaluation\n",
    "    evalu = {}\n",
    "    for x in range(50):\n",
    "        env.reset()\n",
    "        currentNum = 0\n",
    "        state = (5 * env.agent_pos[0]) + env.agent_pos[1]\n",
    "        action = np.argmax(qTable[state, :])\n",
    "        done = False\n",
    "        totalreward=0\n",
    "        while not done and currentNum < env.max_timesteps:\n",
    "            obs, reward, done, info = env.step(action)\n",
    "            totalreward += reward\n",
    "            nextAction = np.argmax(qTable[state])\n",
    "            state = (5 * env.agent_pos[0]) + env.agent_pos[1]\n",
    "            action = nextAction \n",
    "            currentNum += 1\n",
    "        evalu[x] = totalreward\n",
    "    return qTable, arr, epsilonDic, evalu\n"
   ]
  },
